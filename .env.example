# Shopify Configuration
# Get these from your Shopify Admin > Settings > Apps and sales channels > Develop apps
REACT_APP_SHOPIFY_API_URL=https://your-store.myshopify.com/api/2024-01/graphql.json
REACT_APP_SHOPIFY_STOREFRONT_TOKEN=your-shopify-storefront-access-token-here

# EmailJS Configuration
# Get these from https://dashboard.emailjs.com/
REACT_APP_EMAILJS_SERVICE_ID=service_panbfjb
REACT_APP_EMAILJS_TEMPLATE_ID=your-emailjs-admin-template-id
REACT_APP_EMAILJS_THANKYOU_TEMPLATE_ID=your-emailjs-thankyou-template-id
REACT_APP_EMAILJS_PUBLIC_KEY=your-emailjs-public-key
REACT_APP_TO_EMAIL=admin@ireksapiary.com

# Stripe Configuration (if using direct Stripe integration)
# Get these from https://dashboard.stripe.com/apikeys
REACT_APP_STRIPE_PUBLISHABLE_KEY=pk_test_your-stripe-publishable-key

# Backend Server Configuration (if using custom backend)
REACT_APP_BACKEND_URL=http://localhost:5000

# Firebase Configuration (if using Firebase)
# Get these from Firebase Console > Project Settings
REACT_APP_FIREBASE_API_KEY=your-firebase-api-key
REACT_APP_FIREBASE_AUTH_DOMAIN=your-project.firebaseapp.com
REACT_APP_FIREBASE_PROJECT_ID=your-project-id
REACT_APP_FIREBASE_STORAGE_BUCKET=your-project.appspot.com
REACT_APP_FIREBASE_MESSAGING_SENDER_ID=your-sender-id
REACT_APP_FIREBASE_APP_ID=your-app-id
